<!-- views/urls_index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
  <div class="container">
        <!-- make a call to the header file in the partials folder -->
    <%- include('partials/_header') %>
  </div>

  <h1>Websites</h1>
  <ul>
    <% for (let urlKey in urls) { %>
      <li><%= urlKey %> - <%= urls[urlKey] %></li>
    <% } %>
  </ul>
  <h2>URL Shortening Link</h2>
  <h3><a href="">Future useful link</a></h3>

  <form method="POST" action="/urls/9sm5xK/delete">
  <input type="submit" value="Submit">
  </form>

</body>
</html>